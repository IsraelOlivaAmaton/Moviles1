<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".carrito">


    <TextView
        android:id="@+id/inicio_tv_titulo"
        android:layout_width="194dp"
        android:layout_height="43dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="109dp"
        android:gravity="center"
        android:text="@string/car_tv1"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/inicio_iv_sillon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/inicio_ib_agregar"
        android:layout_width="76dp"
        android:layout_height="58dp"
        android:layout_marginStart="342dp"
        android:layout_marginBottom="59dp"
        android:contentDescription="@string/car_des_iv2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/inicio_tv_producto"
        android:layout_width="304dp"
        android:layout_height="68dp"
        android:layout_marginTop="46dp"
        android:layout_marginBottom="21dp"
        android:paddingStart="20dp"
        android:text="@string/car_tv2"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/inicio_iv_sillon"
        app:layout_constraintTop_toBottomOf="@+id/inicio_tv_titulo"
        tools:ignore="MissingConstraints,RtlSymmetry" />

    <ImageView
        android:id="@+id/inicio_iv_sillon"
        android:layout_width="94dp"
        android:layout_height="66dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="104dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="25dp"
        android:contentDescription="@string/car_des_iv1"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toStartOf="@+id/inicio_tv_producto"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/inicio_sillon"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="308dp"
        android:layout_height="67dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="21dp"
        android:layout_marginBottom="353dp"
        android:paddingStart="20dp"
        android:paddingTop="20dp"
        android:text="@string/car_tv3"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/inicio_ib_agregar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/inicio_tv_producto"
        tools:ignore="MissingConstraints,RtlSymmetry" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="88dp"
        android:layout_height="73dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="463dp"
        android:contentDescription="@string/car_des_iv3"
        android:paddingTop="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inicio_iv_sillon"
        app:srcCompat="@drawable/inicio_macetero"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>